# Backend Environment Variables
NODE_ENV=development
PORT=4001

# PostgreSQL
DB_HOST=127.0.0.1
DB_PORT=5433
DB_NAME=sayitownit
DB_USER=sayitownit
DB_PASSWORD=your_password_here

# Ollama (Local)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_USERNAME=
OLLAMA_PASSWORD=
WHISPER_MODEL=huggingface.co/xkeyC/whisper-large-v3-turbo-gguf:latest
LLM_MODEL=llama3.1:8b

# Groq API (for Whisper transcription fallback when YouTube captions unavailable)
# Get your key at: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# YouTube Transcript API
# Get your key at: https://www.youtube-transcript.io
YOUTUBE_TRANSCRIPT_API_KEY=your_youtube_transcript_api_key_here

# Processing
AUDIO_CHUNK_SECONDS=30
ANALYSIS_BATCH_CHUNKS=5

# Temp directory for audio processing
TEMP_DIR=./temp
